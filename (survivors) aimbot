 — 29/09/2025 1:03 am
--// Shift TeleportWalk (stops when releasing Shift)
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

local speed = 0.3 -- adjust this to change teleport speed
local teleporting = false

RunService.Heartbeat:Connect(function(delta)
    if not character or not humanoid or humanoid.Parent == nil then
        return
    end

    if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
        teleporting = true
        if humanoid.MoveDirection.Magnitude > 0 then
            character:TranslateBy(humanoid.MoveDirection * (delta * 10 * speed))
        end
    else
        -- Shift not held, stop teleportwalk
        if teleporting then
            teleporting = false
        end
    end
end)

-- Respawn handling
player.CharacterAdded:Connect(function(char)
    character = char
    humanoid = character:WaitForChild("Humanoid")
end)
