loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
wait(0.2)
-- Variables
local lp = game.Players.LocalPlayer
local path = game.Workspace.Enemies
local UserInputService = game:GetService("UserInputService")
getgenv().Farm = false
local distance = 13 -- Adjustable distance
-- Farming loop
local function BringNPCs()
    while getgenv().Farm do
        local playerHRP = lp.Character and lp.Character:FindFirstChild("HumanoidRootPart")
        if playerHRP then
            for _, npc in ipairs(path:GetChildren()) do
                local npcHRP = npc:FindFirstChild("HumanoidRootPart")
                local npcHumanoid = npc:FindFirstChild("Humanoid")
                if npcHRP and npcHumanoid and npcHumanoid.Health > 0 then
                    npcHRP.CFrame = playerHRP.CFrame + playerHRP.CFrame.LookVector * distance
                end
            end
        end
        task.wait(0.1)
    end
end
-- Keybind Toggle (T)
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == Enum.KeyCode.G then
        getgenv().Farm = not getgenv().Farm
        if getgenv().Farm then
            print("Farm Enabled")
            task.spawn(BringNPCs)
        else
            print("Farm Disabled")
        end
    end
end)
